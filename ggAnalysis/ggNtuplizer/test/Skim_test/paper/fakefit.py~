import ROOT as r

lowX=0.21
lowY=0.70
lumi  = r.TPaveText(lowX, lowY+0.06, lowX+0.15, lowY+0.16, "NDC")
lumi.SetTextFont(61)
lumi.SetTextSize(0.08)
lumi.SetBorderSize(   0 )
lumi.SetFillStyle(    0 )
lumi.SetTextAlign(   12 )
lumi.SetTextColor(    1 )
lumi.AddText("CMS Preliminary")
r.gStyle.SetOptStat(0)
r.gStyle.SetOptFit(1111)
c=r.TCanvas('c','',900,700)
f=r.TFile('QCDFakein_QCD.root')
deno= f.Get('deno')
neo=f.Get('neo')
neo.Divide(deno)
neo.Draw()
#c.Print('QCDFake.eps')
neo.SetMarkerStyle(20)
neo.SetMarkerSize(0.7)
neo.GetXaxis().SetTitle("#tau pt")
neo.GetYaxis().SetTitle("Fake Rate (%)")
neo.SetTitle('')
neo.SetName('Fake Rate')
neo.Draw()
func0 = r.TF1("func0","[2]+[0]*TMath::Exp([1]*x)",10,150)
func0.SetParameters(0,.2)
func0.SetParameters(1,-.004)
neo.Fit("func0","","", 20, 150)
r.gStyle.SetOptFit(11111)
lumi.Draw()
c.Print('QCDFake.pdf')
